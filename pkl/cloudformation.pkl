open class Resource {
    Type: String
    Properties: Any?
    CreationPolicy: Map? 
    DeletionPolicy: String?
    DependsOn: Listing<String>?
    Metadata: Map?
    UpdatePolicy: Map?
    UpdateReplacePolicy: String?
    function Ref(to: String): Mapping = new Mapping {
        ["Ref"] = to
    }
}

open class Parameter {
    Type: String
    Default: String?
    AllowedValues: Listing<String>?
    Description: String?
}

